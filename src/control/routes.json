{
    "index": {
        "destination": "pages/index.php",
        "js": "generic.js"
    },
    "pet/add": {
        "destination": "pages/add_pet.php",
        "js": "add_pet.js"
    },
    "pet/:id": {
        "destination": "pages/animal.php",
        "js": "animal.js"
    },
    "user/:username": {
        "destination": "pages/profile.php",
        "js": "profile.js"
    },
    "signin": {
        "destination": "pages/login.php",
        "js": "generic.js"
    },
    "signup": {
        "destination": "pages/register.php",
        "js": "register.js"
    },
    "discover": {
        "destination": "pages/discover.php",
        "js": "discover.js"
    },
    "pets": {
        "destination": "pages/pet_management.php",
        "js": "pet_management.js"
    },
    "404": {
        "destination": "pages/404.php",
        "js": "generic.js"
    },

    "api/user": {
        "destination": "control/api/user.php"
    },
    "api/user/:listParam/:csrf": {
        "destination": "control/api/user.php"
    },
    "api/user/:listParam": {
        "destination": "control/api/user.php"
    },
    "api/existence/:type/:value": {
        "destination": "control/api/existence.php"
    },
    "api/search/:what/:specie/:color/:size/:value": {
        "destination": "control/api/search.php"
    },
    "api/pet/:what/:arg1": {
        "destination": "control/api/pet.php"
    },
    "api/pet": {
        "destination": "control/api/pet.php"
    },
    "api/pet/:size": {
        "destination": "control/api/pet.php"
    },
    "api/pet/comment": {
        "destination": "control/api/post.php"
    },
    "api/adoption/:pet/:csrf": {
        "destination": "control/api/adoption.php"
    },
    "api/adoption/:pet": {
        "destination": "control/api/adoption.php"
    },
    "api/adoption/:pet/:adopter/:csrf": {
        "destination": "control/api/adoption.php"
    },


    "action/signin": {
        "destination": "control/actions/login.php"
    },
    "action/signup": {
        "destination": "control/actions/register.php"
    },
    "action/signout": {
        "destination": "control/actions/logout.php"
    },
    "action/addPet": {
        "destination": "control/actions/add_pet.php"
    },
    "action/updatePet": {
        "destination": "control/actions/update_pet.php"
    }
}